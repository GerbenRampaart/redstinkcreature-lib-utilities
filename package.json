{
  "name": "@redstinkcreature/lib-utilities",
  "version": "0.1.0",
  "description": "Base utilities project for utils, build and docs",
  "author": "Gerben Rampaart",
  "private": true,
  "license": "UNLICENSED",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "binary": "bun build ./src/main.ts --compile --target=bun --outfile=./dist/app",
    "build": "bun build ./src/main.ts --sourcemap --target=bun --outfile=./dist/app.js",
    "format": "deno fmt",
    "start": "bun run --debug ./src/main.ts",
    "prod": "bun run ./dist/app.js",
    "lint": "deno lint",
    "test": "bun --inspect test",
    "repl": "bun run ./src/util/repl.ts",
    "ncu": "ncu -u -p bun",
    "publish": "jsr publish",
    "docs:types": "mkdir -p ./dist/docs/types && ts-to-jsdoc -p ./tsconfig.json --out ./dist/docs/types"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.6",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.7",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.7",
    "@nestjs/microservices": "^10.3.7",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/platform-socket.io": "^10.3.7",
    "@nestjs/websockets": "^10.3.7",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "app-root-path": "^3.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "mqtt": "^5.5.0",
    "nats": "^2.21.0",
    "nestjs-pino": "^4.0.0",
    "pino": "^8.20.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "ts-to-jsdoc": "^2.1.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "jsr": "^0.12.1",
    "npm-check-updates": "^16.14.18",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.7",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "supertest": "^6.3.4",
    "@types/bun": "latest",
    "typescript": "^5.4.4",
    "tsconfig-paths": "^4.2.0"
  }
}